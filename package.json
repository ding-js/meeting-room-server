{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "egg": "^2.2.1",
    "egg-passport": "^2.0.1",
    "egg-scripts": "^2.5.0",
    "egg-sequelize": "^3.1.4",
    "moment": "^2.22.2",
    "mysql2": "^1.5.3",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "prestart": "npm run migrate:up",
    "start": "egg-scripts start",
    "stop": "egg-scripts stop --title=egg-server-server",
    "predev": "npm run migrate:up",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "migrate:new": "egg-sequelize migration:create",
    "migrate:up": "egg-sequelize db:migrate",
    "migrate:down": "egg-sequelize db:migrate:undo",
    "db":
      "docker container rm mysql -f && docker run --name mysql -e MYSQL_ROOT_PASSWORD=root -e MYSQL_DATABASE=meeting_room -p 3306:3306 mysql:5"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Yuchao Ding",
  "license": "MIT"
}
